defines                 = Q_QDOC \
                          QT_.*_SUPPORT \
                          QT_.*_LIB \
                          QT_COMPAT \
                          QT_KEYPAD_NAVIGATION \
			  QT_NO_EGL \
                          QT3_SUPPORT \
                          Q_WS_.* \
                          Q_OS_.* \
                          Q_BYTE_ORDER \
			  QT_DEPRECATED \
                          Q_NO_USING_KEYWORD \
                          __cplusplus

versionsym              = QT_VERSION_STR

codeindent              = 1

# Files not referenced in any qdoc file (last four needed by qtdemo)
# See also qhp.Qt.extraFiles
extraimages.HTML        = qt-logo \
                          trolltech-logo \
			  bg_l.png \
                          bg_l_blank.png \
                          bg_r.png \
			  box_bg.png \
			  breadcrumb.png \
			  bullet_gt.png \
                          bullet_dn.png \
			  bullet_sq.png \
                          bullet_up.png \
                          feedbackground.png \
                          horBar.png \
			  page.png \
			  page_bg.png \
			  sprites-combined.png \
			  spinner.gif \
                          taskmenuextension-example.png \
                          coloreditorfactoryimage.png \
                          dynamiclayouts-example.png \
                          stylesheet-coffee-plastique.png

# This stuff is used by the new doc format.
scriptdirs              = $QT_SOURCE_TREE/doc/src/template/scripts
styledirs               = $QT_SOURCE_TREE/doc/src/template/style

scripts.HTML		= functions.js \
			  jquery.js

styles.HTML		= style.css \
			  style_ie6.css \
			  style_ie7.css \
			  style_ie8.css
